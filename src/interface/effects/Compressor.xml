<!DOCTYPE fxtemplate SYSTEM "Compressor">
<double id="threshold" text="Threshold" value="-10" suffix="dB" min="-50" max="0" step="10"></double>
<double id="ratio" text="Ratio" value="2" min="1" max="50" step="1"></double>
<double id="attack" text="Attack time" value="100" suffix="ms" min="10" max="1000" step="10"></double>
<double id="release" text="Release time" value="100" suffix="ms" min="10" max="1000" step="10"></double>
<enum id="type" text="Compression type" value="upward">
